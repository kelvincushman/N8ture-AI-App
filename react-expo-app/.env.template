# N8ture AI Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_YOUR_KEY_HERE

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=YOUR_PROJECT_ID.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=YOUR_PROJECT_ID
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=YOUR_PROJECT_ID.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=YOUR_MESSAGING_SENDER_ID
EXPO_PUBLIC_FIREBASE_APP_ID=YOUR_APP_ID

# Google Gemini API (Backend Only - Managed by Cloud Functions)
# This should NOT be exposed to the client
# GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_URL=http://localhost:5001/YOUR_PROJECT_ID/us-central1

# EAS Configuration
# Get this from: https://expo.dev/accounts/[account]/projects/[project]/settings
EAS_PROJECT_ID=YOUR_EAS_PROJECT_ID
